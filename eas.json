{
  "cli": {
    "version": ">= 7.4.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_KEY": "${API_KEY}",
        "AUTH_DOMAIN": "${AUTH_DOMAIN}",
        "PROJECT_ID": "${PROJECT_ID}",
        "STORAGE_BUCKET": "${STORAGE_BUCKET}",
        "MESSAGING_SENDER_ID": "${MESSAGING_SENDER_ID}",
        "APP_ID": "${APP_ID}"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
